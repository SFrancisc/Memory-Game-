<UserControl x:Class="Memory.MemoryCard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Memory"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="100">
    <Grid x:Name="grid_Card">
        <Frame x:Name="frame_MemoryCard" HorizontalAlignment="Center" VerticalAlignment="Center" Height="150" Width="100" NavigationUIVisibility="Hidden"/>
    </Grid>
</UserControl>
